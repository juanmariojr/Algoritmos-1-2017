using namespace std;

void main()
{int numero, divisor = 2, cont = 0;
 cout<<"\n\n Ingrese un numero entero: ";
 cin>>numero;

 while(divisor < numero)
    {if(numero % divisor == 0)
        {cont = cont + 1;
        }
     divisor = divisor + 1;
    }

 if(cont > 0)
    {cout<<"\n\n El numero ingresado no es primo";
    }
 else
    {cout<<"\n\n El numero ingresado es primo";
    }
 getch();
}
